# Assure IDP - Environment Variables Example
# Copy this file to .env and fill in your values

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_secret_key_here
VITE_S3_BUCKET_NAME=assure-idp-documents

# ServiceNow Configuration
VITE_SNOW_INSTANCE=dev12345.service-now.com
VITE_SNOW_USERNAME=your_username
VITE_SNOW_PASSWORD=your_password
VITE_SNOW_CLIENT_ID=your_oauth_client_id
VITE_SNOW_CLIENT_SECRET=your_oauth_client_secret

# API Configuration
VITE_API_BASE_URL=https://api.yourbackend.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_BATCH_PROCESSING=false

# Application Settings
VITE_APP_NAME=DXC Assure IDP
VITE_APP_VERSION=1.0.0
VITE_APP_VENDOR=DXC Technology
VITE_LOG_LEVEL=info
